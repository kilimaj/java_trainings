<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Student">
	<insert id="insert" parameterType="Student" useGeneratedKeys="true" keyProperty="regno">
		insert into student(stdname,total,dob) values(#{stdname},#{total},#{dob})
	</insert>
		
</mapper>